{
    "version": 2,
    "public": true,
    "routes": [
        { "src": "/", "dest": "/api/basic" },
        { "src": "/(.*)", "dest": "/api/advanced" }
    ],
    "functions": {
        "api/**/*.ts": {
            "memory": 128,
            "runtime": "now-deno@0.4.0"
        }
    },
    "env": {
        "DENO_VERSION": "1.0.3"
    }
}
